	public static void relatedProdDisplay() throws Exception {
		WebdriverUtils.goToURL(Environments.getURL());
		WebdriverUtils
				.goToURL("https://www.jellybelly.com/very-cherry-jelly-beans-16-oz-re-sealable-bag/p/9000_VRY_CHERRY");
